<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
                        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="com.innovaee.eorder.action.cash" namespace="/cash"
        extends="struts-base">
        <result-types>
            <result-type name="tiles"
                class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="cash" class="com.innovaee.eorder.action.cash.CashAction"
            method="cash">
            <result name="error" type="tiles">cash</result>
            <result name="input" type="tiles">cash</result>
            <result name="success" type="tiles">cash</result>
        </action>

        <action name="edit" class="com.innovaee.eorder.action.cash.CashAction"
            method="edit">
            <result name="error" type="tiles">cashEdit</result>
            <result name="input" type="tiles">cashEdit</result>
            <result name="success" type="tiles">cashEdit</result>
        </action>

        <action name="list" class="com.innovaee.eorder.action.cash.CashAction"
            method="list">
            <result name="error" type="tiles">cashEdit</result>
            <result name="input" type="tiles">cashEdit</result>
            <result name="success" type="tiles">cashEdit</result>
        </action>

        <action name="discount" class="com.innovaee.eorder.action.cash.CashAction"
            method="discount">
            <result name="error" type="tiles">cashEdit</result>
            <result name="input" type="tiles">cashEdit</result>
            <result name="success" type="tiles">cashEdit</result>
        </action>

        <action name="save" class="com.innovaee.eorder.action.cash.CashAction"
            method="save">
            <result name="error" type="tiles">cashEdit</result>
            <result name="input" type="tiles">cashEdit</result>
            <result name="success" type="tiles">cash</result>
        </action>

        <action name="print" class="com.innovaee.eorder.action.cash.CashAction"
            method="print">
            <result name="error" type="tiles">print</result>
            <result name="input" type="tiles">print</result>
            <result name="success" type="tiles">print</result>
        </action>


    </package>

</struts>
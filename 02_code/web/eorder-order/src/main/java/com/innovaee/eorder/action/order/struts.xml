<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
                        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="com.innovaee.eorder.action.order" namespace="/order"
        extends="struts-base">
        <result-types>
            <result-type name="tiles"
                class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="order" class="com.innovaee.eorder.action.order.OrderAction"
            method="order">
            <result name="error" type="tiles">order</result>
            <result name="input" type="tiles">order</result>
            <result name="success" type="tiles">order</result>
        </action>

        <action name="list" class="com.innovaee.eorder.action.order.OrderAction"
            method="list">
            <result name="error" type="tiles">order</result>
            <result name="input" type="tiles">order</result>
            <result name="success" type="tiles">order</result>
        </action>

        <action name="abc" class="com.innovaee.eorder.action.order.OrderAction"
            method="abc">
            <result name="error" type="tiles">order</result>
            <result name="input" type="tiles">order</result>
            <result name="success" type="tiles">order</result>
        </action>

    </package>

</struts>